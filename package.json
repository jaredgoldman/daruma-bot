{
  "name": "darumabot",
  "version": "1.1.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "git clean -xdf --exclude=\"/config/**/*\"",
    "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
    "commands:register": "npm run build && node -r dotenv/config dist/core/deployCommands.js",
    "dev": "nodemon",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
    "start": "npm run start:bot",
    "start:bot": "npm run build && node -r dotenv/config --enable-source-maps dist/index.js",
    "test": "jest --watch --verbose --coverage",
    "test:dev": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "dependencies": {
    "@discordjs/builders": "^1.3.0",
    "@discordjs/rest": "^1.3.0",
    "algosdk": "^1.22.0",
    "async-exit-hook": "^2.0.1",
    "axios": "^1.1.3",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "ioredis": "^5.2.3",
    "mongodb": "^4.10.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1"
  },
  "devDependencies": {
    "@types/async-exit-hook": "^2.0.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^8.0.1",
    "ioredis-mock": "^8.2.2",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
